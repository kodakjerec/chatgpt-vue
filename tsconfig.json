{
  "extends": "@vue/tsconfig/tsconfig.web.json",
  "include": ["env.d.ts", "src/**/*", "src/**/*.vue"],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    },
    "resolveJsonModule": true,
    "noImplicitAny": false,
    "noImplicitThis": false,
    "lib": [
      "ESNext",
      "DOM"
    ],
    "typeRoots": [
      "./node_modules/@types",
      "./src/types"
    ]
  },
  "references": [
    {
      "path": "./tsconfig.config.json"
    }
  ]
}
